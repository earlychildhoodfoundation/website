{
  "name": "earlychildhoodfoundation.org",
  "version": "2.0.0",
  "description": "This repository contains the source code for the website of the Caplan Foundation for Early Childhood. The foundation is dedicated to improving the quality of early childhood education and development for children from birth through age seven.",
  "main": "index.js",
  "scripts": {
    "build": "npx @11ty/eleventy --quiet && npx tailwindcss -i ./src/assets/styles/styles.css -o ./www/assets/styles/styles.css --minify",
    "serve": "npx @11ty/eleventy --serve --incremental --quiet",
    "debug": "node --inspect node_modules/.bin/eleventy --serve --incremental",
    "css": "npx tailwindcss -i ./src/assets/styles/styles.css -o ./www/assets/styles/styles.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-bundle": "^1.0.5",
    "@directus/format-title": "^10.1.1",
    "@tailwindcss/typography": "^0.5.12",
    "js-yaml": "^4.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-jinja-template": "^1.3.3",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.3",
    "title-case-commonjs": "^4.3.3"
  }
}
